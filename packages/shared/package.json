{
  "name": "@lmms-lab/writer-shared",
  "version": "0.1.0",
  "description": "Shared types and utilities for LaTeX Writer",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/node": "^25.0.10",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3"
  }
}
