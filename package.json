{
  "name": "agentic-latex-writer",
  "version": "0.1.0",
  "private": true,
  "description": "AI-native LaTeX editor - let Claude, Cursor, and Codex write your papers while you focus on research",
  "author": "EvolvingLMMs Lab",
  "license": "MIT",
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "typecheck": "turbo typecheck",
    "clean": "turbo clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\""
  },
  "devDependencies": {
    "prettier": "^3.5.0",
    "turbo": "^2.4.0",
    "typescript": "^5.7.0"
  }
}
